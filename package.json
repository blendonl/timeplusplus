{
  "name": "timeplusplus",
  "displayName": "time++",
  "description": "tacks time",
  "publisher": "blendonluta",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "timeplusplus.refreshEntry",
          "when": "view == timeplusplus",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "timeplusplus",
          "name": "Time++"
        }
      ]
    },
    "commands": [
      {
        "command": "timeplusplus.refreshEntry",
        "title": "Refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.3.5",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "tsc": "^2.0.3"
  }
}
